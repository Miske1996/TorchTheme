<!doctype html>
<html class="no-js" lang="{{ request.locale.iso_code }}">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <meta name="theme-color" content="">
    <script src="https://unpkg.com/@lottiefiles/lottie-player@latest/dist/lottie-player.js"></script>

    <link rel="canonical" href="{{ canonical_url }}">
    <link
      rel="preload"
      as="font"
      href="{{ 'Lato-Thin.woff2' |  asset_url }}"
      type="font/woff2"
      crossorigin>
    <link
      rel="preload"
      as="font"
      href="{{ 'Lato-Light.woff2' | asset_url }}"
      type="font/woff2"
      crossorigin>
    <link
      rel="preload"
      as="font"
      href="{{ 'Lato-Regular.woff2' | asset_url }}"
      type="font/woff2"
      crossorigin>
    <link
      rel="preload"
      as="font"
      href="{{ 'Lato-Black.woff2' | asset_url }}"
      type="font/woff2"
      crossorigin>
    <link
      rel="preload"
      as="font"
      href="{{ 'Lato-Bold.woff2' | asset_url }}"
      type="font/woff2"
      crossorigin>
    <title>Torch Theme</title>
    {{ content_for_header }}
    {{ 'global.css' | asset_url | stylesheet_tag }}
  </head>

  <body>
    {% section 'header-section' %}
    <main class="main_content_layout">{{ content_for_layout }}</main>
    {% render 'footer-component' %}
    <script src="{{'animation.js' | asset_url}}"></script>
  </body>

</html>